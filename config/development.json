{
    "logging": false,
    "imageFetcher": {
        /*
         * Remember to escape backslashes in RegExps as \\ in JSON files
         * or else the config parser will fail.
         */
        "urlWhitelist": {
            "localhost": "^.+:\/\/localhost",
            "127.0.0.1": "^.+:\/\/127.0.0.1",
            "::1": "^.+:\/\/::1"
        },
        "cache": {
        }
    },
    "imageConverter": {
        "paddingColor": "#FFBBBB" //imagemagicks default shade of gray for borders
    }
}
